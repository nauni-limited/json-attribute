{
  "name": "nauni-limited/json-attribute",
  "description": "A PHP 8 package for working with JSON data",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Tamas Dobo",
      "email": "tamas@nauni.co.uk"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^0.12",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-phpunit": "^0.12",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Nauni\\JSON\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nauni\\JSON\\Tests\\": "tests/"
    }
  }
}
